<h2>Artists</h2>

<div>
  <% artists.forEach(function(artist) { %>
    <h2>
    <a href="/artists/<%= artist.id %>" class="artistName"><%= artist.name %>
    </h2>
    <img class="artistImg" src="<%= artist.imageUrl %>">
    </a>
    <br>
    <p>
    <%= artist.description %>
    </p>
    <hr />
  <% }) %>
</div>
